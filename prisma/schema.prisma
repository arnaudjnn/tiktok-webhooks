generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Webhook {
  id        String   @id @default(cuid())
  sessionId String
  endpoint  String
  lastEventId  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

